{
  "name": "react-express-typescript",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build:web": "cross-env NODE_ENV=production webpack --config scripts/webpack/build.js",
    "start:web": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack/dev.js",
    "build:server": "cross-env NODE_ENV=production tsc -p server/tsconfig.json",
    "start:server": "cross-env NODE_ENV=development ts-node server/index.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webpack-env": "^1.18.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.2"
  }
